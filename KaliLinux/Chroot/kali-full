#!/bin/sh

cd $HOME
###
wget -O $PREFIX/bin/nethunter https://raw.githubusercontent.com/xiv3r/Termux-Pentesting-Distro/refs/heads/main/KaliLinux/Chroot/nethunter
###
cp -r $PREFIX/bin/nethunter $PREFIX/bin/kali
###
chmod 700 $PREFIX/bin/nethunter
###
chmod 700 $PREFIX/bin/kali
###
wget -O kali-full.tar.xz https://kali.download/nethunter-images/current/rootfs/kali-nethunter-rootfs-full-arm64.tar.xz
###
echo "
[*] Extracting rootfs...
"
###
bsdtar -xpJf kali-full.tar.xz > /dev/null 2>&1
###
echo "
[*] Done!
"
###
echo "
To Login ðŸ‘‡
user: "kali"
root: "kali -r"
"
